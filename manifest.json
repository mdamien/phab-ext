{
  "name": "Phab",
  "version": "0.1",
  "permissions": [
    "<all_urls>"
  ],
  "icons": {
    "16": "icon.png",
    "128": "icon.png"
  },
  "background" : {
    "scripts" : ["jquery.js", "background.js"]
  },
  "browser_action": {
      "default_title": "Phabricator",
      "default_icon": "icon.png",
      "default_popup": "popup.html"
  },
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'"
}
